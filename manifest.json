{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "ac19.png",
         "38": "ac38.png"
      },
      "default_popup": "popup.html",
      "default_title": "AutoCorrect for Google Chrome(TM)"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "engine.js" ],
      "matches": [ "<all_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "Autocorrects text while you type in most text fields. Still very experimental.",
   "icons": {
      "128": "ac128.png",
      "16": "ac16.png",
      "19": "ac19.png",
      "32": "ac32.png",
      "39": "ac39.png",
      "48": "ac48.png",
      "64": "ac64.png"
   },
   "incognito": "spanning",
   "manifest_version": 2,
   "name": "AutoCorrect for Google Chrome(TM)",
   "offline_enabled": true,
   "options_page": "options.html",
   "permissions": [ "<all_urls>", "unlimitedStorage", "tabs" ],
   "version": "1.1"
}
